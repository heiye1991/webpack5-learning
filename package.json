{
  "name": "webpack5-learning",
  "version": "1.0.0",
  "description": "Learning webpack version 5",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack --config ./entry-output/config/webpack.config.js",
    "webpack2": "webpack --config ./pack-resources/config/webpack.config.js",
    "webpack3:dev": "cross-env NODE_ENV=development webpack --config ./styles-extract/config/webpack.config.js",
    "webpack3:build": "cross-env NODE_ENV=production webpack --config ./styles-extract/config/webpack.config.js",
    "webpack4:dev": "cross-env NODE_ENV=development webpack --config ./pack-output/config/webpack.config.js",
    "webpack4:build": "cross-env NODE_ENV=production webpack --config ./pack-output/config/webpack.config.js",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heiye1991/webpack5-learning.git"
  },
  "keywords": [
    "webpack5"
  ],
  "author": "heiye1991<1604043458@qq.com>",
